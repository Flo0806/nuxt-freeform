{
  "name": "nuxt-freeform-root",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter nuxt-freeform dev",
    "dev:prepare": "pnpm --filter nuxt-freeform dev:prepare",
    "build": "pnpm --filter nuxt-freeform build",
    "docs:dev": "pnpm --filter nuxt-freeform-docs dev",
    "docs:build": "pnpm --filter nuxt-freeform-docs build",
    "docs:preview": "pnpm --filter nuxt-freeform-docs preview",
    "lint": "pnpm --filter nuxt-freeform lint",
    "test": "pnpm --filter nuxt-freeform test",
    "release": "pnpm build && changelogen --release && pnpm --filter nuxt-freeform publish && git push --follow-tags"
  },
  "devDependencies": {
    "@types/node": "latest",
    "changelogen": "^0.6.2"
  },
  "packageManager": "pnpm@10.11.0"
}
